#include<stdio.h>
#include<iostream>
using namespace std;
int main(void){
    int num = 10;
    int *ptr = &num;
    int  *ptr2 = ptr;
     printf("%d ",(*ptr)++);
     printf("%d ",(*ptr2)++);
     cout <<  num << ' ';
cout <<  &num << ' ';
      cout <<  ptr << ' ';
 cout<<            *ptr << ' ';
 cout<<            ptr2<< ' ';
cout<<            *ptr2 << ' ';
return 0;
}

